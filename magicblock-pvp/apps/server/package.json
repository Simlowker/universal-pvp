{
  "name": "@magicblock-pvp/server",
  "version": "1.0.0",
  "description": "MagicBlock PvP Game Backend Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:backup": "./scripts/db/backup.sh",
    "db:restore": "./scripts/db/restore.sh",
    "db:migration": "tsx scripts/db/migration.ts"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.49.1",
    "@opentelemetry/exporter-prometheus": "^0.49.1",
    "@opentelemetry/instrumentation-express": "^0.38.0",
    "@opentelemetry/instrumentation-http": "^0.49.1",
    "@opentelemetry/sdk-node": "^0.49.1",
    "@prisma/client": "^5.11.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.8.0",
    "@sentry/profiling-node": "^10.8.0",
    "@sentry/tracing": "^7.120.4",
    "@solana/web3.js": "^1.91.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.0",
    "pg": "^8.11.0",
    "prom-client": "^14.2.0",
    "prometheus-api-metrics": "^3.2.2",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.2.0",
    "socket.io": "^4.7.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.0",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prisma": "^5.11.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "keywords": [
    "magicblock",
    "pvp",
    "game",
    "blockchain",
    "solana"
  ],
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
