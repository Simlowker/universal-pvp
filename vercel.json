{
  "name": "universal-pvp-devnet",
  "version": 2,
  "builds": [
    {
      "src": "magicblock-pvp/apps/web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "magicblock-pvp/apps/server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "magicblock-pvp/apps/server/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "magicblock-pvp/apps/web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SOLANA_NETWORK": "devnet",
    "NEXT_PUBLIC_SOLANA_RPC_HOST": "https://api.devnet.solana.com",
    "NEXT_PUBLIC_SOLANA_WS_HOST": "wss://api.devnet.solana.com",
    "NEXT_PUBLIC_STRATEGIC_DUEL_PROGRAM_ID": "6gaBQmha1o6u26PWb9quyzhcxufGWAWBYr71m6e6F3GD",
    "NODE_ENV": "production"
  },
  "functions": {
    "magicblock-pvp/apps/server/src/index.ts": {
      "maxDuration": 30
    }
  }
}